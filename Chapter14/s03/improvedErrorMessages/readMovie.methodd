	static Movie readMovie(BufferedReader br) {		String name;		int    playingTime;		Movie  newMovie;		name = br.readLine();		if (name==null)			return null;		try {			playingTime = Integer.parseInt(br.readLine());		} catch (NumberFormatException e) {			System.err.println("Bad playingTime format for "+name);			throw e;		}		newMovie = new Movie(name,playingTime);		return newMovie;	}