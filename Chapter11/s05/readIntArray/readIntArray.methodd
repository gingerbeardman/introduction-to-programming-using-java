	//  Read integers (one per line) from br and store in a[first], ..., a[beyond21];	//  Read until end of file or until no more elements are available.	//  Return the number of integers read and stored.	int  readIntArray(int[]  a,  int  first,  int  beyond,  BufferedReader  br)  {		if  (first==beyond)			return  0;		String  s  =  br.readLine();		if  (s==null)			return  0;		a[first]  =  Integer.parseInt(s);		return 1+readIntArray(a,first+1,beyond,br);	}