	static void permutations(Vector v, Set dict, String p, String letters){		if  (letters.length()==0)  {			if  (dict.contains(p))				v.addElement(p);			return;		}		int    i;		i  =  0;		while  (i<letters.length())  {			String  newPermutation  =  p + letters.substring(i,i+1);			String  newLetters = letters.substring(0,i) + letters.substring(i+1);			permutations(v,dict,newPermutation,newLetters);			i++;		}	}