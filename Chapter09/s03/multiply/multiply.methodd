	private int multiply(int x, int y) {		int	count,	// count==the number of additions done.			result;	// result==0+x+x+...+x (count times), that is, 				//     result==x*count.		count = 0;		result = 0;		while (count!=y) {			result += x;			count++;		}		// count==y, result==x*y		return result;	}