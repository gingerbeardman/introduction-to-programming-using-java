//------------// Introduction to Programming Using Java: An Object-Oriented Approach//	Arnow/Weiss//------------import java.io.*;class PlayLife {	public static void main(String[] a) throws Exception {		if (a.length != 2)			System.err.println("Usage: java Life <size> <probability of life>");		else {			int	size = Integer.parseInt(a[0]);			double	prob = Double.valueOf(a[1]).doubleValue();			Life	lf = new Life(new LifeDisplay(), size, prob);			lf.doGenerations();		}	}}